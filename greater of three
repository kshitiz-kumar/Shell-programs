echo "enter first number"

read first

echo "enter second number"

read second

echo "enter third number"

read third

if [ $first -gt $second ]

then 

 if [ $first -gt $third ] 
 then
   echo "$first is greatest"
 else
    echo "$third is greatest"
  fi				 
fi

if [ $second -gt $first ]

then 

 if [ $second -gt $third ] 
 then
   echo "$second is greatest"
 else
    echo "$third is greatest"
  fi				 
fi


